# Cấu hình mặc định cho Whisper
# Copy file này thành .env hoặc chỉnh sửa trực tiếp

# Whisper model size
# Options: tiny, base, small, medium, large
WHISPER_MODEL=large

# Language code cho video
# vi = Tiếng Việt
# en = English
# ja = Japanese
# zh = Chinese
# None = Auto detect
WHISPER_LANGUAGE=vi

# Device to use
# cuda = GPU (nhanh hơn)
# cpu = CPU (chậm hơn nhưng không cần GPU)
# None = Auto detect
WHISPER_DEVICE=

# Skip existing transcripts
WHISPER_SKIP_EXISTING=true
